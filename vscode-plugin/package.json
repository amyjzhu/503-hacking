{
  "name": "codemap",
  "displayName": "codemap",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:codemap.d3",
    "onCommand:codemap.server"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codemap.d3",
        "title": "Displays the visualization of your source code"
      },
      {
        "command": "codemap.server",
        "title": "Launch or connect to the background server to receive the data"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
